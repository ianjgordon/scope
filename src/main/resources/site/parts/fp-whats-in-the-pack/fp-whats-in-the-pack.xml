<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part>
  <display-name>FP What's in the pack</display-name>
  <config>
    <input type="TextLine" name="heading">
      <label>Heading</label>
      <occurrences minimum="1" maximum="1"/>
      <default>What's in the pack?</default>
    </input>
    <item-set name="feature">
      <label>Feature</label>
      <items>
        <input name="heading" type="TextLine">
          <label>Heading</label>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input name="text" type="TextLine">
          <label>Text</label>
          <occurrences minimum="1" maximum="1"/>
        </input>
      </items>
      <immutable>false</immutable>
      <occurrences minimum="1" maximum="0"/>
    </item-set>
    <item-set name="button">
      <label>Button</label>
      <items>
        <inline mixin="link-button"/>
      </items>
      <occurrences minimum="0" maximum="1"/>
    </item-set>
  </config>
</part>