<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<part>
  <display-name>FP Other Families</display-name>
  <config>
    <input type="TextLine" name="heading">
      <label>Heading</label>
      <occurrences minimum="1" maximum="1"/>
      <default>What do other families think?</default>
    </input>
    <item-set name="quote">
      <label>Quote</label>
      <items>
        <input name="text" type="TextLine">
          <label>Text</label>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input name="name" type="TextLine">
          <label>Name</label>
          <occurrences minimum="1" maximum="1"/>
        </input>
      </items>
      <immutable>false</immutable>
      <occurrences minimum="1" maximum="0"/>
    </item-set>
  </config>
</part>